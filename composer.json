{
  "name": "dimichspb/yii2-notificator",
  "description": "Yii2 notificator component",
  "keywords": ["yii2", "extension", "notifications"],
  "homepage": "https://github.com/dimichspb/yii2-notificator",
  "type": "yii2-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "Dmitry Tarantin",
      "email": "dimichspb@gmail.com"
    }
  ],
  "require": {
    "yiisoft/yii2": "*",
    "yiisoft/yii2-queue": "*",
    "beberlei/assert": "^2.8",
    "ramsey/uuid": "^3.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^6",
    "yiisoft/yii2-swiftmailer": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "dimichspb\\yii\\notificator\\": "src/",
      "dimichspb\\yii\\notificator\\tests\\": "tests/"
    }
  },
  "extra": {
    "bootstrap": "dimichspb\\yii\\notificator\\Bootstrap"
  }
}
